; Example ini, needs to be placed in webroot

[general]
email_admin=${email_admin}

[content]
default_post_image=https://api.zaplog.pro/content/dark-background-3494082_1280.jpg
broken_link_redirector=${ZAPLOG_GOTO_URL}

[translation]
auto_translate=${auto_translate}
deepl_auth_key=${deepl_auth_key}
deepl_api_url=https://api-free.deepl.com/v2/translate

[memcache]
memcache_host=${MEMCACHED_HOST}
memcache_port=${MEMCACHED_PORT}

[cors]
cors_origin[]=
cors_origin[]=
cors_expose_headers=
cors_max_age=1728000
cors_allow_credentials=1
cors_allow_methods=POST,GET,PUT,DELETE,PATCH
cors_allow_headers=Authorization,Content-Type

[database]
database_host=${ZAPLOG_DB_HOST}
database_name=${ZAPLOG_DB_NAME}
database_user=${ZAPLOG_DB_USER}
database_password=${ZAPLOG_DB_PASSWORD}
database_charset=utf8
database_query_logging=${ZAPLOG_DB_QUERY_LOGGING}

[smtp]
smtp_host=${ZAPLOG_MAIL_HOST}
smtp_login=${ZAPLOG_MAIL_LOGIN}
smtp_password=${ZAPLOG_MAIL_PASSWORD}
smtp_secure=TLS
smtp_port=587

[email]
email_sender=${ZAPLOG_MAIL_SENDER_EMAIL}
email_sendername=${ZAPLOG_MAIL_SENDER_NAME}
