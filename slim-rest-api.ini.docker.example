; Example ini, needs to be placed in webroot

[memcache]
memcache_host=${MEMCACHED}
memcache_port=11211

[cors]
cors_origin[]=
cors_origin[]=
cors_expose_headers=
cors_max_age=1728000
cors_allow_credentials=1
cors_allow_methods=POST,GET,PUT,DELETE,PATCH
cors_allow_headers=Authorization,Content-Type

[database]
database_host=${ZAPLOG_DB_HOST}
database_name=${ZAPLOG_DB_NAME}
database_user=${ZAPLOG_DB_USER}
database_password=${ZAPLOG_DB_PASSWORD}
database_charset=utf8

[smtp]
smtp_host=
smtp_login=
smtp_password=
smtp_secure=TLS
smtp_port=587

[email]
email_sender=
email_sendername=Zaplog
